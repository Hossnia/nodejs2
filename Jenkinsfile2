pipeline {
    agent {
        docker {image 'node:14' 
        args '-u root:root'}
    }
    stages{
        stage("Sonarcloud analysis"){
            steps{
                withSonarQubeEnv('sonarqube'){
                    steps{
                       sh '''sonar-scanner \\
                        -Dsonar.organization=hossnia \\
                        -Dsonar.projectKey=Hossnia_nodejs2 \\
                        -Dsonar.sources=. \\
                        -Dsonar.host.url=https://sonarcloud.io'''
                }
            }
        }
    }
}
}