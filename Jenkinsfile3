pipeline{
    agent any
    stages{
        stage("sonarqube analysis"){
            steps{
                nodejs(nodeJSInstallationName: 'nodejs'){
                    sh 'npm install'
                    withSonarQubeEnv('sonarqube'){
                        sh 'npm install sonar-scanner'
                        sh 'npm run sonar'
                    }
                }
                
            }
        }
    }
}